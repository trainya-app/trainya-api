{
  "name": "trainya-admin-api",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.1.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1"
  },
  "scripts": {
    "dev": "ts-node-dev --exit-child --transpile-only src/index.ts --ignore __tests__",
    "migrate": "npx prisma migrate dev --name init",
    "start": "node dist/src/index.js",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "commit": "cz",
    "test": "npx autocannon -c 500 -d 30 --workers 10 --renderStatusCodes --latency --warmup [ -c 1 -d 2] https://trainya-app-api.herokuapp.com/workoutPlans",
    "postinstall": "npm run build"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^9.1.1",
    "@types/cors": "^2.8.12",
    "@types/node": "^17.0.39",
    "@types/typescript": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "autocannon": "^7.9.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.3",
    "prettier": "^2.6.2",
    "prettier-plugin-prisma": "^3.14.0",
    "prisma": "^3.15.2",
    "ts-jest": "^28.0.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
